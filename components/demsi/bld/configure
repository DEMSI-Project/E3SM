    filepath.write("/lcrc/group/e3sm/ac.akturner/scratch/anvil/dtestdemsi/bld/cmake-bld/demsi/DEMSI/model/build/petsc-install/\n")
    filepath.write("/lcrc/group/e3sm/ac.akturner/scratch/anvil/dtestdemsi/bld/cmake-bld/demsi/DEMSI/model/build/permon-install/\n")
    filepath.write("/lcrc/group/e3sm/ac.akturner/scratch/anvil/dtestdemsi/bld/cmake-bld/demsi/DEMSI/model/build/Icepack-install/\n")
    filepath.write("/lcrc/group/e3sm/ac.akturner/scratch/anvil/dtestdemsi/bld/cmake-bld/demsi/DEMSI/model/build/compadre-install/\n")
    filepath.write("/lcrc/group/e3sm/ac.akturner/scratch/anvil/dtestdemsi/bld/cmake-bld/demsi/DEMSI/model/build/tinyxml2-install/\n")

    filepath.close()

###############################################################################
def _main_func():
