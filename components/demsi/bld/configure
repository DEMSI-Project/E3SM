#!/usr/bin/env python3

def write_filepath():

    filepathName = "Filepath"

    filepath = open(filepathName,"w")

    filepath.write("/lcrc/group/e3sm/ac.akturner/scratch/anvil/dtestdemsi/bld/cmake-bld/demsi/DEMSI/model/build/src-install/\n")
